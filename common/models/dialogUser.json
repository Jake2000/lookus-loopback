{
  "name": "dialoguser",
  "base": "PersistedModel",
  "idInjection": true,
  "properties": {
    "dnd_start":{
      "type": "date",
      "required": false,
      "description": "Время начала режима 'не беспокоить'"
    },
    "dnd_life_time":{
      "type": "number",
      "required": false,
      "description": "Кол-во минут в режиме 'не беспокоить'"
    },
    "unread_count": {
      "type": "number",
      "required": false,
      "default": 0,
      "description": "Кол-во непрочитанных сообщений в диалоге для пользователя"
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "user_id"
    },
    "dialog": {
      "type": "belongsTo",
      "model": "dialog",
      "foreignKey": "dialog_id"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": []
}
