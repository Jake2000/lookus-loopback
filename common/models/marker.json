{
  "name": "marker",
  "plural": "markers",
  "base": "PersistedModel",
  "idInjection": true,
  "properties": {
    "life_time": {
      "type": "number",
      "required": true,
      "description": "Время жизни маркера в минутах"
    },
    "type": {
      "type": "number",
      "required": true,
      "description": "Тип маркера"
    },
    "text": {
      "type": "string",
      "required": true,
      "description": "Текст маркера (становится заголовком диалога для маркера)"
    },
    "image_url": {
      "type": "string",
      "description": "Изображение маркера"
    },
    "image_preview_url": {
      "type": "string",
      "description": "Изображение маркера (уменьш.)"
    },
    "created": {
      "type": "date",
      "description": "Дата и время создания"
    },
    "updated": {
      "type": "date",
      "description": "Дата и время изменения"
    },
    "deleted": {
      "type": "date",
      "description": "Дата и время удаления"
    },
    "is_up": {
      "type": "boolean",
      "description": "Поднят ли маркер"
    },
    "location": {
      "type": "locationModel",
      "description": "Широта и Долгота",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "dialog": {
      "type": "hasOne",
      "model": "dialog",
      "foreignKey": "marker_id"
    },
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "user_id"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": []
}
