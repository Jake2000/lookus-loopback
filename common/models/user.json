{
  "name": "user",
  "plural": "users",
  "base": "User",
  "idInjection": true,
  "properties": {
    "first_name": {
      "type": "string",
      "required": true,
      "description": "Имя"
    },
    "last_name": {
      "type": "string",
      "required": true,
      "description": "Фамилия"
    },
    "birthday": {
      "type": "date",
      "description": "Дата рождения"
    },
    "sex": {
      "type": "number",
      "required": true,
      "description": "Пол - (0-не указано, 1-мужчина, 2-женщина)"
    },
    "image_preview_url": {
      "type": "string",
      "required": false,
      "description": "Ссылка на аватар (уменьшенный)"
    },
    "image_url": {
      "type": "string",
      "required": true,
      "description": "Ссылка на аватар"
    },
    "country": {
      "type": "string",
      "description": "Страна (текст)"
    },
    "city": {
      "type": "string",
      "description": "Город (текст)"
    },
    "status": {
      "type": "string",
      "required": false,
      "description": "Статус пользователя"
    },
    "vk_id": {
      "type": "number",
      "required": false,
      "description": "ID пользователя в Вконтакте"
    },
    "is_online": {
      "type": "boolean",
      "required": false,
      "description": "Онлайн ли пользователь"
    },
    "is_friend": {
      "type": "boolean",
      "required": false,
      "description": "Является ли пользователь другом"
    },
    "created": {
      "type": "date",
      "description": "Дата и время создания"
    }
  },
  "validations": [],
  "relations": {
    "identities": {
      "type": "hasMany",
      "model": "userIdentity",
      "foreignKey": "userId"
    },
    "credentials": {
      "type": "hasMany",
      "model": "userCredential",
      "foreignKey": "userId"
    },
    "dialogs": {
      "type": "hasMany",
      "model": "dialog",
      "foreignKey": "user_id",
      "through": "dialoguser"
    },
    "friends": {
      "type": "hasMany",
      "model": "user",
      "foreignKey": "friend_id",
      "through": "friend"
    },
    "owners": {
      "type": "hasMany",
      "model": "user",
      "foreignKey": "owner_id",
      "through": "friend"
    },
    "settings": {
      "type": "hasOne",
      "model": "settings",
      "foreignKey": "user_id"
    },
    "marker": {
      "type": "hasOne",
      "model": "marker",
      "foreignKey": "user_id"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "current"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "loginVKCallback"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "loginVK"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "loginFB"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "loginFBCallback"
    }
  ],
  "methods": []
}
